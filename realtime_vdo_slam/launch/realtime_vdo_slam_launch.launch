<launch>

	<arg name="config_file" default="realtime_vdo.yaml"/>
  <!-- <include file="$(find python_service_starter)/launch/python_service_starter.launch"/> -->
  <rosparam command="load" file="$(find realtime_vdo_slam)/config/$(arg config_file)"/>
  <param name="frame_id" value="vdo_camera_link" />
  <param name="global_optim_trigger" value="100" />
  <!-- <param name="skip_frames" value="5" /> -->

  <node pkg="realtime_vdo_slam" type="ros_vdoslam_node" name="ros_vdoslam" output="screen">
    <!-- Remap if needed from vdo generated topics to vdo_slam expected topics. These are the same as the ImageProcessing node -->
    <!-- <remap from="/camera/rgb/image_raw" to="/camera/rgb/image_raw"/>
    <remap from="/camera/mask/image_raw" to="/camera/mask/image_raw"/>
    <remap from="/camera/flow/image_raw" to="/camera/flow/image_raw"/>
    <remap from="/camera/depth/image_raw" to="/camera/depth/image_raw"/> -->
  </node>
</launch>